{
  "routes": [
  {
      "route": "/school/*",
      "serve": "/school.html",
      "statusCode": 200,
      "rewrite": "/school.html"
    },
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["portfolio/*", "/images/*", "/css/*", "/js/*", "*.js", "*.css"]
  }
}